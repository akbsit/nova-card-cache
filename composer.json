{
    "name": "falbar/nova-card-cache",
    "description": "Cleaning application cache from laravel nova dashboard card.",
    "keywords": [
        "laravel",
        "nova",
        "cache",
        "file",
        "redis",
        "flush"
    ],
    "homepage": "http://falbar.ru/",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0",
        "laravel/nova": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Falbar\\NovaCardCache\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Falbar\\NovaCardCache\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
