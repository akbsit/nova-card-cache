{
  "name": "akbsit/nova-card-cache",
  "description": "Cleaning application cache from laravel nova dashboard card.",
  "keywords": [
    "laravel",
    "nova",
    "cache",
    "file",
    "redis",
    "flush"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0|^10.0|^11.0",
    "laravel/nova": "^3.0|^4.0"
  },
  "autoload": {
    "psr-4": {
      "Akbsit\\NovaCardCache\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Akbsit\\NovaCardCache\\CardServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
